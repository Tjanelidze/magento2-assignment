<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
    <table name="inventory_shipment_source" resource="default" engine="innodb">
        <column xsi:type="int" name="shipment_id" unsigned="true" nullable="false" identity="false" />
        <column xsi:type="varchar" name="source_code" nullable="false" />
        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="shipment_id"/>
        </constraint>
    </table>
</schema>
